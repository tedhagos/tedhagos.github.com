---
layout: 
---
<?xml version="1.0" encoding="utf-8"?>

<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Ted's blog'</title>
 <link href="http://tedhagos.com/feed/" rel="self"/>
 <link href="http://tedhagos.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://tedhagos.com/</id>
 <author>
   <name>Ted Hagos</name>
   <email>tedhagos@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title | xml_escape }}</title>
   <link href="http://tedhagos.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://tedhagos.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>